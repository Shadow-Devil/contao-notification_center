        <target>Sprache ID %s editieren.</target>
      </trans-unit>
      <trans-unit id="tl_nc_language.copy.0">
        <source>Copy language</source>
        <target>Sprache kopieren</target>
      </trans-unit>
      <trans-unit id="tl_nc_language.copy.1">
